{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "Realtime": {
    "RenewTime": "00:00:20",
    "ConfigPath": "Processes.json"
  },
  "Retro": {
    "Name": "Mongo Reader",
    "Type": "mongo",
    "Connection": "mongodb://localhost:27017"
  },

  "DiagnosticExplorer": {
    "Uri": "http://localhost:2803/diagnostics",
  // "Uri": "http://drdiagnostics:2803/diagnostics",
    "Enabled": "true"
  }
}